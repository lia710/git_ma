package ex06;

import java.util.Iterator;

//3. 1+(-2)+3+(-4)+...과 같은 식으로 계속 더해나갔을때, 몇까지 더해야 총합이 100이상이 되는지 구하시오
public class test03 {
public static void main(String[] args) {
	
	for (int i = 0; i >= 0; i+=2) {
	for (int j = 0; j <= 0; j-=2) {
		System.out.println(i+j);
		break;
	}
	
			
		}
		 
		
	}
}

