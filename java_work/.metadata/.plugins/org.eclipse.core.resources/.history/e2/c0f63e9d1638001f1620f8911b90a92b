package ex04;

public class ex3 {

	public static void main(String[] args) {
		int num1 = 10;
		
		/*
		 * 브레이크포인트 . 더블클릭
		 * 벌레모양으로 실행
		 * f6 한줄씩 진행
		 * f8 종료
		 * */
		
		// num++
		// num = num+1
		// num += 1
		
		System.out.println(num1++);
		System.out.println(++num1);
		System.out.println(num1);
		
		System.out.println(num1--);
		System.out.println(--num1);
		System.out.println(num1);
	}
}
