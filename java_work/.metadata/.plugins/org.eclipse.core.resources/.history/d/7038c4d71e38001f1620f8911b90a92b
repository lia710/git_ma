package ex04;

import java.util.Scanner;

public class ex06 {

	// 숫자를 입력받아
	// 10 미만이면 10미만 출력
	// 10 이상이고 20이하이면 10이상20이하 출력
	// 20 초과이면 20초과 출력
	
	public static void main(String[] args) {

		Scanner scan = new Scanner(System.in);
		
		System.out.println("숫자 입력");
		int num1 = scan.nextInt();
		
		System.out.println(num1);
	
	
}
}
