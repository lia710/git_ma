package ex06;
//1. 1부터 20까지의 정수 중에서 2또는 3의 배수가 아닌 수의 총합을 구하시오
public class test01 {
public static void main(String[] args) {
	int num=0;
	 while (num<20) {
		num++;
		boolean num1= (num %2 ! = 0);
		boolean num2= (num %3 ! = 0);
		
		if (num1 && num2) {
			int sum = num1 + num2;
			System.out.println(sum);
		}
	}
	}
	 
}
